# =============================================================================
# OpenClaw environment variables template
# Copy to openclaw-secrets.env, fill in your values.
# The setup script places this at ~/.openclaw/.env
# =============================================================================

# ── AI Provider API Keys ─────────────────────────────────────────────────────
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
OPENROUTER_API_KEY=
GEMINI_API_KEY=

# ── OpenClaw Internal ────────────────────────────────────────────────────────
# Auto-generated if left blank
OPENCLAW_GATEWAY_TOKEN=
OPENCLAW_BROWSER_CONTROL_TOKEN=

# ── Channel Tokens ───────────────────────────────────────────────────────────
# Discord: https://discord.com/developers/applications → Bot → Token
DISCORD_TOKEN=

# Telegram: message @BotFather on Telegram → /newbot
TELEGRAM_BOT_TOKEN=

# Slack: see scripts/create-slack-bot.sh or https://api.slack.com/apps
SLACK_BOT_TOKEN=
SLACK_APP_TOKEN=

# ── Search & Services ────────────────────────────────────────────────────────
BRAVE_SEARCH_API_KEY=

